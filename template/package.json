    "turbo-darwin-arm64": "1.11.1",
    "turbo-linux-64": "1.11.1",
    "turbo-linux-arm64": "1.11.1",
    "turbo-windows-64": "1.11.1",
    "turbo-windows-arm64": "1.11.1"
  }
