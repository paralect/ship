    "@tabler/icons-react": "3.10.0",
    "@tanstack/react-query": "5.50.1",
    "@tanstack/react-table": "8.19.2",
    "app-constants": "workspace:*",
    "app-types": "workspace:*",
    "axios": "1.7.2",
    "clsx": "2.1.1",
    "dayjs": "1.11.10",
    "dotenv-flow": "4.1.0",
    "lodash": "4.17.21",
