  "devDependencies": {
    "@tanstack/react-query-devtools": "5.50.1",
    "@types/lodash": "4.17.6",
    "@types/mixpanel-browser": "2.49.0",
    "@types/node": "22.10.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "eslint": "8.56.0",
    "eslint-config-custom": "workspace:*",
    "lint-staged": "15.2.7",
    "postcss": "8.4.47",
    "postcss-preset-mantine": "1.17.0",
    "postcss-simple-vars": "7.0.1",
    "prettier": "3.3.2",
    "prettier-config-custom": "workspace:*",
    "stylelint": "16.10.0",
    "stylelint-config-standard-scss": "13.1.0",
    "tsconfig": "workspace:*",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "bash -c 'tsc --noEmit'",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
