    "storybook-dark-mode": "4.0.2",
    "stylelint": "16.10.0",
    "stylelint-config-standard-scss": "13.1.0",
    "tsconfig": "workspace:*",
    "typescript": "5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "bash -c 'tsc --noEmit'",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "!(*.css|*.ts|*.tsx)": [
      "prettier --write"
    ]
  }
