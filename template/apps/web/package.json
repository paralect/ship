{
  "name": "web",
  "version": "1.0.0",
  "description": "Next.js web",
  "author": "Paralect",
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3002",
    "start": "next start -p 3002",
    "tsc": "tsc --noEmit --watch",
