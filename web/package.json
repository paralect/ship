{
  "name": "react-starter",
  "version": "1.2.0",
  "description": "Koa react",
  "private": false,
  "main": "src/server/app.js",
  "author": "Paralect",
  "license": "MIT",
  "keywords": [
    "koa",
    "react",
    "redux",
    "paralect"
  ],
  "scripts": {
    "build-client": "webpack -p --config src/client/webpack.config.js",
    "start": "NODE_ENV=production node src/server/app.js",
    "test": "run-s test:**",
    "test:server": "eslint ./src/server",
    "test:client": "eslint  --ext js --ext jsx ./src/client",
    "test:lint-styles": "stylelint \"src/client/**/*.pcss\"",
    "development": "NODE_ENV=development nodemon --watch src/server --watch src/client/webpack.dev.config.js src/server/app.js",
    "format": "prettier-eslint --write \"src/**/*.?(js|jsx)\""
  },
  "dependencies": {
    "@paralect/common-logger": "0.1.10",
