    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "handlebars": "4.0.11",
    "history": "4.7.2",
    "html-webpack-plugin": "2.30.1",
    "incstr": "1.2.3",
    "joi-browser": "13.0.1",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.0",
    "koa-hbs": "1.0.0",
    "koa-logger": "3.1.0",
    "koa-mount": "3.0.0",
    "koa-router": "7.3.0",
